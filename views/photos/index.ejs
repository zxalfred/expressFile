<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Express stock photo application.</p>
      <% photos.forEach((photo) => { %>
        <div class="photo">
          <h2><%= photo.name %></h2>
          <img src='<%= photo.path %>' />
        </div>
      <% }) %>
  </body>
</html>
